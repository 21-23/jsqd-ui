{
  "name": "jsqd-ui",
  "version": "1.2.0",
  "main": "app/index.js",
  "license": "LGPL-3.0",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=development LOG_LEVEL=all webpack-dev-server --config webpack.config.dev.js",
    "build": "cross-env LOG_LEVEL=all webpack --config webpack.config.dev.js",
    "build:prod": "npm run clean:prod && webpack --config webpack.config.prod.js",
    "build:local": "cross-env LOG_LEVEL=all webpack --config webpack.config.local.js",
    "watch": "cross-env NODE_ENV=development LOG_LEVEL=all webpack --config webpack.config.dev.js --watch",
    "watch:local": "cross-env NODE_ENV=development LOG_LEVEL=all webpack --config webpack.config.local.js --watch",
    "lint": "eslint ./app && stylint ./app",
    "precommit": "lint-staged",
    "clean:prod": "rm -rf dist-prod"
  },
  "devDependencies": {
    "babel-core": "^6.22.1",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-react-jsx": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "cross-env": "^3.1.4",
    "css-loader": "^0.26.1",
    "eslint": "^3.14.0",
    "favicons-webpack-plugin": "^0.0.7",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.26.0",
    "husky": "^0.13.0",
    "lint-staged": "^3.2.7",
    "resolve-url-loader": "^1.6.1",
    "style-loader": "^0.13.1",
    "stylint": "^1.5.9",
    "stylint-loader": "^1.0.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.4.0",
    "webpack": "^3.4.1",
    "webpack-dev-server": "^2.6.1",
    "webpack-merge": "^2.6.1",
    "webpack-notifier": "^1.5.0"
  },
  "dependencies": {
    "codemirror": "^5.23.0",
    "js-beautify": "^1.6.8",
    "message-factory": "github:21-23/message-factory#v6.4.0",
    "phoenix": "github:21-23/phoenix#v2.1.1",
    "preact": "^7.1.0",
    "preact-compat": "^3.16.0",
    "preact-redux": "^2.0.0",
    "react-rnd": "^5.0.9",
    "redux": "^3.6.0",
    "redux-actions": "^1.2.1",
    "steno": "21-23/steno#v1.0.0"
  }
}
